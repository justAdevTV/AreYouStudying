{
  "manifest_version": 2,

  "name": "Are You Studying?",
  "description": "Reduces the risk of distractions!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms",
    "https://ajax.googleapis.com/"
  ],
  
    "background": {
      "scripts": ["js/background.js"]
    }
}
